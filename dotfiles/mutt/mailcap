text/plain; cat %s; copiousoutput
text/html; mkdir -p /tmp/mutt/$(id -u) \; cp %s /tmp/mutt/$(id -u) \; xdg-open /tmp/mutt/$(id -u)/$(basename %s) &
text/html; w3m -dump %s; copiousoutput; nametemplate=%s.html
image/*; mkdir -p /tmp/mutt/$(id -u) \; cp %s /tmp/mutt/$(id -u) \; xdg-open /tmp/mutt/$(id -u)/$(basename %s) &
